def main():
    n = int(input())
    lista = []
    cont = 0
    
    while cont < n:
        num =int(input())
        lista.append(num)
        cont += 1
    
    comp = len(lista)
    contador = 0 
    i = 0
    j = -1
     
    while i < comp and j >= -comp:
        if lista[i] == lista[j]:
            contador += 1
        i += 1
        j -= 1
            
    if contador == comp:
        print('SIM')
    elif contador != comp:
        print('NAO')
    
    
main()
